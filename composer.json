{
    "name": "preemstudio/laravel-conformist",
    "description": "A package to easily build beautiful API SDKs with Laravel.",
    "keywords": [
        "PreemStudio",
        "laravel",
        "conformist"
    ],
    "homepage": "https://github.com/preemstudio/laravel-conformist",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@preem.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^9.4",
        "illuminate/http": "^9.4",
        "illuminate/support": "^9.4",
        "spatie/laravel-data": "^3.1",
        "spatie/laravel-package-tools": "^1.14",
        "spatie/regex": "^3.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.29",
        "graham-campbell/analyzer": "^3.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26",
        "spatie/pest-plugin-snapshots": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "PreemStudio\\Conformist\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
